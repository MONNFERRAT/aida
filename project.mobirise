        {
          "_styles": {
            "padding-top": "(@paddingTop * 15px)",
            "padding-bottom": "(@paddingBottom * 15px)",
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value"
            },
            ".mbr-overlay": {
              "background-color": "@overlayColor",
              "opacity": "@overlayOpacity",
              "z-index": "1",
              "pointer-events": "none"
            },
            ".carousel": {
              "position": "relative",
              ".container": {
                "position": "absolute",
                "top": "80%!important",
                "-webkit-transform": "translateY(-50%) !important",
                "transform": "translateY(-50%) !important",
                "padding": "60px",
                "left": "0",
                "right": "0",
                "margin": "0",
                "z-index": "1"
              }
            },
            ".mbr-section-title": {
              "margin": ".5rem 0",
              "@media (max-width: 662px)": {
                "margin": "0"
              }
            },
            "amp-carousel": {
              "width": "100%",
              "object-fit": "cover",
              "height": "400px"
            },
            "img, amp-img": {
              "height": "100%",
              "width": "100%",
              "object-fit": "contain"
            },
            ".slider-box": {
              "position": "relative",
              "width": "100%",
              "overflow": "hidden",
              "height": "100%"
            },
            ".amp-carousel-button-prev": {
              "left": "80px"
            },
            ".amp-carousel-button-next": {
              "right": "80px"
            },
            "@media (max-width: 991px)": {
              ".amp-carousel-button-prev": {
                "left": "13px"
              },
              ".amp-carousel-button-next": {
                "right": "13px"
              }
            },
            "@media (max-width: 767px)": {
              ".amp-carousel-button-prev": {
                "top": "360px"
              },
              ".amp-carousel-button-next": {
                "top": "360px"
              }
            },
            ".iconfont-wrapper": {
              "margin-right": "(@iconsSize * 0.1rem)",
              "@media (max-width: 991px)": {
                "margin-bottom": "30px"
              }
            },
            ".amp-iconfont": {
              "font-size": "(@iconsSize * 0.5rem)",
              "width": "(@iconsSize * 0.5rem)",
              "color": "@primaryColor"
            },
            "& when not (@showTag)": {
              ".title-box > .mbr-jc-b": {
                "justify-content": "flex-end"
              }
            },
            ".progress-wrap": {
              "height": "6px",
              "width": "100%",
              "border-radius": "3px"
            },
            ".progress": {
              "height": "100%",
              "border-radius": "3px"
            },
            ".progress1": {
              "width": "~\"@{progress1}%\"",
              "background-color": "@progress1Color"
            },
            ".progress2": {
              "width": "~\"@{progress2}%\"",
              "background-color": "@progress2Color"
            },
            ".progress3": {
              "width": "~\"@{progress3}%\"",
              "background-color": "@progress3Color"
            },
            ".progress4": {
              "width": "~\"@{progress4}%\"",
              "background-color": "@progress4Color"
            },
            ".progress5": {
              "width": "~\"@{progress5}%\"",
              "background-color": "@progress5Color"
            },
            ".progress-value1:before": {
              "content": "\"@{progress1}\""
            },
            ".progress-value2:before": {
              "content": "\"@{progress2}\""
            },
            ".progress-value3:before": {
              "content": "\"@{progress3}\""
            },
            ".progress-value4:before": {
              "content": "\"@{progress4}\""
            },
            ".progress-value5:before": {
              "content": "\"@{progress5}\""
            },
            ".progress-box span": {
              "color": "@countColor"
            }
