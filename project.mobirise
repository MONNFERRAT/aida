        {
          "alias": false,
          "_styles": {
            "& when not (@fullScreen)": {
              "padding-top": "(@paddingTop * 15px)",
              "padding-bottom": "(@paddingBottom * 15px)"
            },
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value"
            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)",
              ".mbr-overlay": {
                "background-color": "@overlayColor",
                "opacity": "@overlayOpacity"
              }
            },
            "& when (@transparentBg)": {
              "amp-accordion section": {
                "background": "transparent !important",
                "& > *": {
                  "border": "none"
                }
              }
            },
            "& when not (@showIcons)": {
              ".amp-iconfont": {
                "display": "none !important"
              }
            },
            "& when (@showTitle), (@showSubtitle)": {
              ".mbr-row.text-row": {
                "padding-bottom": "2rem",
                "justify-content": "~\"@{textAlign}\""
              }
            },
            ".mbr-row.toggle-row": {
              "justify-content": "~\"@{togglesAlign}\""
            },
            ".mbr-section-subtitle": {
              "color": "#a2a0a0",
              "padding": "0",
              "margin": "0"
            },
            ".section-text": {
              "padding": "2rem",
              "background": "@bodyBg",
              "& when (@transparentBg)": {
                "background": "transparent !important"
              }
            },
            ".section-title": {
              "padding": "1rem 2rem",
              "background": "@headBg",
              "border-color": "@borderColor",
              "& when (@transparentBg)": {
                "background": "transparent !important"
              },
              "&:focus": {
                "outline": "none"
              }
            },
            ".iconfont-wrapper": {
              "position": "relative",
              "margin-right": "1rem",
              "display": "inline-block",
              "width": "~\"@{iconsSize}rem\"",
              "height": "~\"@{iconsSize}rem\""
            },
            ".amp-iconfont": {
              "font-size": "~\"@{iconsSize}rem !important\""
            },
            "amp-accordion section[expanded] .plus-sign": {
              "display": "none"
            },
            "amp-accordion section:not([expanded]) .minus-sign": {
              "display": "none"
            },
            "@media (max-width: 767px)": {
              ".section-title, .mbr-text": {
                "padding": "1rem",
                "text-align": "left"
              },
              ".amp-iconfont": {
                "padding-left": "1rem"
              }
            }
