                "-ms-transform": "translateY(-20px)",
                "-o-transform": "translateY(-20px)",
                "transform": "translateY(-20px)"
              }
            },
            "& when not (@transparentBg)": {
              ".card-wrapper:after": {
                "content": "\"\"",
                "position": "absolute",
                "left": "0",
                "right": "0",
                "top": "0",
                "bottom": "0",
                "border-radius": "15px",
                "background-color": "@cardBg",
                "opacity": "@bgOpacity",
                "z-index": "-1"
              }
            },
            ".card-box": {
              "width": "100%"
            },
            "& when not (@flexDirection)": {
              "& when (@checkText)": {
                ".card-img": {
                  "min-width": "(@countersWidth * 1%)",
                  "text-align": "center",
                  "@media (min-width: 992px)": {
                    "margin-right": "2rem"
                  },
                  "@media (max-width: 991px)": {
                    "width": "100%",
                    "margin-bottom": "2rem"
                  }
                }
              }
            },
            ".mbr-section-title": {
              "color": "#1bc2ca"
            }
